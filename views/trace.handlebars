version: 1
config:
    timeUnitMillis: 1
events:
    {{#each players}}
    -
        player: {{name}}
        type: join
        time: 0
    {{/each}}
    -
        player: moderator
        type: start
        time: 300
    {{#each words}}
    -
        player: {{player}}
        type: word
        word: {{word}}
        time: {{time}}
    {{/each}}
    -
        player: moderator
        type: stop
        time: {{end_time}}
